function string buscar(string cadena, int fich, int aux)	private		int pf;		string cadena2,cadf,nfich;	end	begin		switch(fich)			case -1:				nfich="config.yaw";			end			case 0:				nfich="dll/config/maintitle.yaw";			end			case 1:				nfich="dll/config/map"+aux+".yaw";			end		end		pf=fopen(nfich,O_READ);		if(pf)			while(!feof(pf))				cadena2=fgets(pf);				if(find(cadena2,cadena)>-1)					cadf=fgets(pf);					break;				end			end			fclose(pf);			log("Buscado "+cadf);			return cadf;		end	end 